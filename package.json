    "build": "babel src --out-dir dist --copy-files --extensions '.ts,.js'",
    "test": "jest",
    "record-tests": "jest -u",
    "check-types": "tsc",
    "download": "npm run build && node --optimize_for_size --max_old_space_size=${MAX_OLD_SPACE_SIZE:-4096} dist/scripts/download.js",
